{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@asset/*": ["src/assets/*"],
      "@images/*": ["src/assets/images/*"],
      "@styles/*": ["src/assets/styles/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@utils/*": ["src/utils/*"],
      "@helpers/*": ["src/utils/helpers/*"],
      "@constants/*": ["src/utils/constants/*"],
      "@types/*": ["src/types/*"]
    }
  }
}